  "bugs": {
    "url": "https://github.com/rishabh09/dash/issues"
  },
  "homepage": "https://github.com/rishabh09/dash#readme",
  "dependencies": {
    "dotenv": "^4.0.0",
    "react": "^16.2.0",
    "react-dom": "^16.2.0",
    "react-redux": "^5.0.6",
    "redux": "^3.7.2",
    "redux-thunk": "^2.2.0"
  },
  "devDependencies": {
    "babel-core": "^6.26.0",
    "babel-loader": "^7.1.2",
    "babel-plugin-transform-class-properties": "^6.24.1",
    "babel-preset-env": "^1.6.1",
    "babel-preset-react": "^6.24.1",
    "coveralls": "^3.0.0",
    "electron": "^1.7.11",
    "enzyme": "^3.2.0",
    "enzyme-adapter-react-16": "^1.1.0",
    "jest-cli": "^21.2.1",
    "react-test-renderer": "^16.2.0",
